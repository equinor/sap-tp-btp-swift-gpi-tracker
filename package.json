{
  "name": "SwiftGPITracker",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap-cloud-sdk/http-client": "^4.1.2",
    "@sap/cds": "^9",
    "@sap/xssec": "^4",
    "express": "^4"
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.13.0",
    "@cap-js/sqlite": "^2",
    "@sap/cds-dk": ">=8",
    "cds-plugin-ui5": "^0.13.0",
    "mbt": "^1.2.29",
    "rimraf": "^5.0.5"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-gpitracker": "cds watch --open gpitracker/index.html?sap-ui-xx-viewCache=false --livereload false",
    "undeploy": "cf undeploy Swift_CAP_GPITracker --delete-services --delete-service-keys --delete-service-brokers",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1",
    "watch-swift_gpi_tracker_app": "cds watch --open swiftgpitrackerapp/index.html?sap-ui-xx-viewCache=false --livereload false",
    "watch-gpitrackerlr": "cds watch --open gpitrackerlr/index.html?sap-ui-xx-viewCache=false --livereload false"
  },
  "cds": {
    "requires": {
      "Swift_CAP_GPITracker": {
        "kind": "rest",
        "credentials": {
          "destination": "Swift-GPI-Tracker-API"
        }
      },
      "gpi.for.Corporates": {
        "kind": "rest",
        "model": "srv/external/SWIFT-API-gpi-corporate_api-7.0.3-swagger"
      },
      "MyService": {
        "kind": "odata-v2",
        "model": "srv/API.cds"
      },
      "[production]": {
        "auth": "xsuaa"
      },
      "connectivity": true,
      "destinations": true,
      "html5-repo": true,
      "portal": true,
      "workzone": true
    }
  },
  "sapux": [
    "app/gpitrackerlr"
  ],
  "workspaces": [
    "app/*"
  ]
}
